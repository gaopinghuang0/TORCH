{
  "name": "TORCH",
  "version": "0.1",
  "description": "A Tool for Organizing Referrencing Contents.",
  "permissions": [
    "tabs",
    // "activeTab",
    "contextMenus",
    "<all_urls>"
  ],
  "minimum_chrome_version": "38",
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery-1.12.3.min.js",
      "js/background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "160": "large-icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["css/content.css"],
      "js": [
        "js/jquery-1.12.3.min.js",
        "js/jquery.highlight.js",
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "TORC",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}